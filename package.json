{
  "name": "empty",
  "version": "0.0.0",
  "private": true,
  "main": "./app/app.js",
  "scripts": {
    "clean": "mkdir dist",
    "build-babel": "babel -d ./dist ./app -s",
    "build": "npm run clean && npm run build-babel",
    "start": "npm run build && node ./dist/app.js",
    "test": "nyc --reporter=html --reporter=text mocha --exit --require babel-core/register",
    "dev": "babel-node ./app/app.js --presets env",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.3",
    "chai-http": "^4.0.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "joi": "^13.4.0",
    "morgan": "~1.9.0",
    "travis-cli": "^1.0.9"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.12.0",
    "faker": "^4.1.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^12.0.2",
    "supertest": "^3.1.0"
  }
}